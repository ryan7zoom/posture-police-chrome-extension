{
    "manifest_version": 3,
    "name": "Posture Police",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },

    "permissions": [
        "alarms",
        "notifications",
        "storage"
    ],

    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}